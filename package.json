{
  "name": "mrof",
  "version": "0.0.0-semantically-released",
  "description":
    "A two-way binding form implementation and give the ability to validate the form inputs.",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "add-contributor": "kcd-scripts contributors add",
    "build": "BUILD_TREESHAKE=true kcd-scripts build --bundle",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s",
    "precommit": "kcd-scripts precommit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "files": ["dist"],
  "keywords": [],
  "author":
    "Jianhua Cheng <w.chengjianhua@gmail.com> (http://chengjianhua.github.io)",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.2.4",
    "ramda": "^0.25.0",
    "recompose": "^0.27.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.3",
    "@storybook/addon-info": "^3.4.3",
    "@storybook/addon-links": "^3.4.3",
    "@storybook/addon-notes": "^3.4.3",
    "@storybook/addon-options": "^3.4.3",
    "@storybook/addons": "^3.4.3",
    "@storybook/react": "^3.4.3",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "kcd-scripts": "^0.36.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true
        }
      ]
    }
  },
  "eslintIgnore": ["node_modules", "coverage", "dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/chengjianhua/mrof.git"
  },
  "bugs": {
    "url": "https://github.com/chengjianhua/mrof/issues"
  },
  "homepage": "https://github.com/chengjianhua/mrof#readme",
  "peerDependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.2"
  }
}
